generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"  // 사용할 DB 종류 (MySQL이면 "mysql"로 변경)
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id @default(uuid())
  email     String  @unique
  password  String
  createdAt DateTime @default(now())
}
